<?xml version="1.0" encoding="utf-8"?>
<manifest package="com.excellence.bouncebackview"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".activity.PullableExpandableListViewActivity">
        </activity>
        <activity android:name=".activity.PullableGridViewActivity">
        </activity>
        <activity android:name=".activity.PullableListViewActivity">
        </activity>
        <activity android:name=".activity.PullableScrollViewActivity">
        </activity>
        <activity android:name=".activity.PullableWebViewActivity">
        </activity>
        <activity android:name=".activity.PullableImageViewActivity">
        </activity>
        <activity android:name=".activity.PullableTextViewActivity">
        </activity>
    </application>

</manifest>